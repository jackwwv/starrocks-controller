global:
    user: "root"
    ssh_port: 22

server_configs:
    fe:
        sys_log_level: "ERROR"
    be:
        sys_log_level: "WARNING"

fe_servers:
  - host: 192.168.230.41
    ssh_port: 22
    http_port: 8030
    rpc_port: 9020
    query_port: 9030
    edit_log_port: 9010
    deploy_dir: /opt/starrocks/fe
    meta_dir: /data/starrocks/fe/meta
    log_dir: /opt/starrocks/fe/log
    priority_networks: 192.168.230.0/24
    config:
      sys_log_level: "INFO"
      sys_log_delete_age: "1d"
